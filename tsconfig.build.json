{
  "extends": "./tsconfig.json",
  "exclude": ["node_modules"],
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "*": ["src/lib/module/*"]
    },
    "sourceMap": false,
    "declaration": true
  },
  "include": ["src/**/*", "index.ts"]
}
